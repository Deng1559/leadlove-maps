{
  "name": "leadlove-credit-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=\"(api|lib|components)\"",
    "test:integration": "jest --testPathPattern=\"integration\"",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:visual": "playwright test --project=visual-chromium",
    "test:a11y": "playwright test --project=accessibility",
    "test:security": "playwright test __tests__/security/",
    "test:performance": "playwright test __tests__/performance/",
    "test:contract": "playwright test __tests__/contract/",
    "test:mobile": "playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "test:all": "npm run test:unit && npm run test:e2e && npm run test:visual && npm run test:a11y && npm run test:security && npm run test:performance && npm run test:contract",
    "test:report": "npm run test:coverage && npm run test:e2e && node __tests__/monitoring/generateReport.js",
    "test:windows": "powershell -ExecutionPolicy Bypass -File ./scripts/run-tests-windows.ps1",
    "playwright:install": "playwright install",
    "playwright:update-snapshots": "playwright test --update-snapshots",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-form": "^0.0.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@stripe/stripe-js": "^7.8.0",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/auth-ui-react": "^0.4.6",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.54.0",
    "@types/node": "^18.19.122",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "csv-parser": "^3.2.0",
    "csv-stringify": "^6.6.0",
    "date-fns": "^3.0.6",
    "google-auth-library": "^9.15.1",
    "googleapis": "^129.0.0",
    "googleapis-common": "^7.2.0",
    "isomorphic-dompurify": "^2.26.0",
    "lucide-react": "^0.303.0",
    "next": "14.0.4",
    "node-html-parser": "^6.1.13",
    "openai": "^4.104.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-query": "^3.39.3",
    "stripe": "^14.25.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@playwright/test": "^1.40.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node-fetch": "^2.6.13",
    "@types/trusted-types": "^2.0.7",
    "@types/uuid": "^9.0.8",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "autoprefixer": "^10.4.16",
    "axe-playwright": "^2.0.1",
    "babel-jest": "^29.7.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "pm2": "^5.3.0",
    "postcss": "^8.4.32",
    "supabase": "^1.142.2",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setup.js"
    ],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/app/layout.tsx",
      "!src/app/not-found.tsx",
      "!src/app/loading.tsx",
      "!src/app/error.tsx",
      "!src/app/global-error.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/",
      "<rootDir>/__tests__/e2e/",
      "<rootDir>/__tests__/visual/",
      "<rootDir>/__tests__/accessibility/",
      "<rootDir>/__tests__/security/",
      "<rootDir>/__tests__/performance/",
      "<rootDir>/__tests__/contract/"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html",
      "json-summary"
    ],
    "collectCoverage": false,
    "verbose": true
  }
}
